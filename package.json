{
  "name": "js-starterkid-pluralsight",
  "version": "1.0.0",
  "description": "Javascript Dev Environment set up pluralsight course",
  "main": "index.js",
  "scripts": {
    "start:message" : "babel-node buildScripts/startMessage.js",
    "prestart": "npm-run-all --parallel start:message",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch":"npm run lint -- --watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch":"npm run test -- --watch",
    "generate-mock-data":"babel-node buildScripts/generateMockData",
    "prestart-mockapi":"npm run generate-mock-data",
    "start-mockapi":"json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkankala/js-starterkid-pluralsight.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkankala/js-starterkid-pluralsight/issues"
  },
  "homepage": "https://github.com/kkankala/js-starterkid-pluralsight#readme",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "3.1.0",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.2",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "0.16.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}